{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "experimentalDecorators": true, 
      "emitDecoratorMetadata": true, 
      "module": "commonjs", 
      "esModuleInterop": true, 
      "forceConsistentCasingInFileNames": true, 
      "strict": true, 
      "skipLibCheck": true,
      "strictPropertyInitialization": false
   }
}
